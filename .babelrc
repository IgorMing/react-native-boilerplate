{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver", {
        "extensions": [".js"],
        "alias": {
          "components": "./src/components",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
